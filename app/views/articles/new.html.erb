<h1> New Article </h1>


<%= form_for :article, url: articles_path do |f| %>
<div>
  <%= f.label :title, 'Title' %>
  <%= f.text_field :title %>
</div>

<div>
  <%= f.label :text, 'Text' %>
  <%= f.text_field :text %>
</div>

<div>
  <%= f.submit 'Send', class: 'btn btn-primary' %>
</div>

<% end %>
